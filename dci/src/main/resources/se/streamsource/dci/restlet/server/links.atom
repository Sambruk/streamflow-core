<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Atom feed</title>
 <link href="$request.resourceRef"/>

#foreach($task in $result.links)
 <entry>
   <title>$task.text</title>
   <link href="$task.href"/>
   <id>urn:uuid:${task.id}</id>
 </entry>
#end

</feed>
